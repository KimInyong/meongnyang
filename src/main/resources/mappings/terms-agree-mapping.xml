<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="termsAgreeDAO">

	
	<select id="selectTermsAgreeList" parameterType="termsAgree">
	
	</select>
	
	<update id="updateTermsAgreeList" parameterType="termsAgree">
		UPDATE terms_agree
		SET
			terms_agree_tb_consent_status = #{termsAgreeTbConsentStatus}
		WHERE
			customer_tb_no = #{customerTbNo}
	</update>

</mapper>
